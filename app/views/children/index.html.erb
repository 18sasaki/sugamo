<p id="notice"><%= notice %></p>

<h1>Children</h1>

<table>
  <thead>
    <tr>
      <th>Unique num</th>
      <th>Class room</th>
      <th>Post number</th>
      <th>Address</th>
      <th>L phone number</th>
      <th>C phone number</th>
      <th>Full name</th>
      <th>Full name f</th>
      <th>Sex code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @children.each do |child| %>
      <tr>
        <td><%= child.unique_num %></td>
        <td><%= child.class_room_id %></td>
        <td><%= child.post_number %></td>
        <td><%= child.address %></td>
        <td><%= child.l_phone_number %></td>
        <td><%= child.c_phone_number %></td>
        <td><%= child.full_name %></td>
        <td><%= child.full_name_f %></td>
        <td><%= child.sex_code %></td>
        <td><%= link_to 'Show', child %></td>
        <td><%= link_to 'Edit', edit_child_path(child) %></td>
        <td><%= link_to 'Destroy', child, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Child', new_child_path %>
